pipeline {
  agent any
  stages {
    stage('myStage'){
      steps {
        git credentialsId: '130a7441-80d1-4afe-a1ec-b78eff09f5bf', url: 'https://github.com/ChrisGroovetechnology/study_jenkins.git/', branch: 'master'
		echo 'Builing'
		sh 'cd study_jenkins'
        sh 'dotnet restore'
		sh 'dotnet test --results-directory ./ --logger "trx;LogFileName=test_results.xml"'
		sh 'dotnet test dotnet test /p:CollectCoverage=true /p:CoverletOutputFormat=opencover'
		sh 'reportgenerator "-reports:study_jenkins.Test/test_results.xml" "-targetdir:coveragereport"'
		sh 'dotnet build'
      }
    }
  }
}